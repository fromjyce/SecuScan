{
  "name": "secuscan",
  "displayName": "SecuScan",
  "description": "A VS Code extension to scan for security vulnerabilities in project dependencies.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "secuscan.checkDependencies",
        "title": "Check Project Dependencies"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode production --config webpack.config.js",
    "watch": "webpack --mode development --watch",
    "lint": "eslint src/**/*.ts src/test/**/*.ts",
    "test": "npx mocha --require ts-node/register --recursive out/test/**/*.js",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/chai": "^4.3.19",
    "@types/glob": "^7.1.2",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "chai": "^5.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "eslint": "^8.57.0",
    "minimatch": "^3.0.4",
    "mocha": "^10.7.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "vscode": "^1.1.37",
    "vscode-test": "^1.6.1",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.5"
  }
}
